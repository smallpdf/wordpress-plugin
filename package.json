		"i18n:make-json": "wp i18n make-json languages --no-purge --pretty-print"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.4.0",
		"sass-loader": "^16.0.3"
	},
	"dependencies": {
		"react": "^18.3.1"
	},
	"files": [
		"build",
		"languages",
		"smallpdf-embed-pdf.php",
		"readme.txt"
	],
